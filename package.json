{
  "type": "module",
  "name": "@etchteam/next-pagination",
  "version": "3.5.5",
  "description": "Pagination component for Next.js apps",
  "author": "etchteam",
  "license": "MIT",
  "repository": "etchteam/next-pagination",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "engines": {
    "node": "^18 || ^20"
  },
  "scripts": {
    "build": "microbundle --no-compress --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react --globals react/jsx-runtime=jsx",
    "start": "npm run build -- watch",
    "prepare": "husky install",
    "test": "CI=true node --experimental-vm-modules ./node_modules/.bin/jest --env=jsdom",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --env=jsdom",
    "predeploy": "cd example && npm install && npm run build && touch build/.nojekyll",
    "deploy": "gh-pages -t -d example/build"
  },
  "peerDependencies": {
    "next": ">=10",
    "react": ">=16"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "4.17.21",
    "query-string": "6.12.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@etchteam/eslint-config": "^1.10.0",
    "@etchteam/stylelint-config": "^1.5.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "gh-pages": "^2.2.0",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "microbundle": "^0.15.1",
    "next": "^13.2.3",
    "node-sass": "9.0.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "1.26.5",
    "semantic-release": "^23.0.0",
    "stylelint": "^15.11.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
