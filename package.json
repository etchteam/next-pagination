{
  "name": "@etchteam/next-pagination",
  "version": "3.2.0",
  "description": "Pagination component for Next.js apps",
  "author": "etchteam",
  "license": "MIT",
  "repository": "etchteam/next-pagination",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build && touch build/.nojekyll",
    "deploy": "gh-pages -t -d example/build"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "next": "^11.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "fibers": "5.0.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.8",
    "next": "^11.1.1",
    "node-sass": "7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^3.4.1",
    "sass": "1.26.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "lodash": "4.17.21",
    "query-string": "6.12.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
